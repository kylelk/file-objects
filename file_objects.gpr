project file_objects is
   for Source_Dirs use ("src");
   for Object_Dir use "obj";
   for Main use ("main.adb");
   
   package Compiler is
    for Default_Switches ("Ada") use ("-O2");
   end Compiler;
   
   package Builder is
      for Executable ("main.adb") use "file_objects";
   end Builder;
end file_objects;